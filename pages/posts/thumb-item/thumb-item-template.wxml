<template name="thumbItem">
  <view class="post-like">
    <view>
      <image wx:if="{{isthumbed}}" class="post-like-image" src="/images/icon/star.png"></image>
      <image wx:else class="post-like-image" src="/images/icon/chat.png"></image>
    </view>

    <text class="post-like-font">{{collection}}</text>
    <image class="post-like-image" src="/images/icon/view.png"></image>
    <text class="post-like-font">{{reading}}</text>
  </view>
</template>